{
   "name":"create-user",
   "active":true,
   "proxy":{
      "preserve_host":false,
      "listen_path":"/users/create",
      "upstreams":{
         "balancing":"roundrobin",
         "targets":[
            {
               "target":"http://customermanagement:8081/users/create"
            }
         ]
      },
      "strip_path":false,
      "append_path":false,
      "methods":[
         "POST",
         "OPTIONS"
      ]
   },
   "plugins":[
      {
         "name":"cors",
         "enabled":true,
         "config":{
            "domains":["*"],
            "exposed_headers":[],
            "methods":[
               "POST",
               "GET",
               "PUT",
               "DELETE",
               "OPTIONS"
            ],
            "request_headers":[]
         }
      }
   ]
}
